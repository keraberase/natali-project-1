{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }
  