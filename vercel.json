{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.html",  
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",  
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",         
            "dest": "/index.html"
        }
    ]
}
