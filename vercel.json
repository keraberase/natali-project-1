{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/images/(.*)",
        "dest": "/images/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      }
    ]
  }
  