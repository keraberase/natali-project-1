{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/images/(.*)",
        "dest": "/dist/images/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/dist/favicon.ico"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }
  